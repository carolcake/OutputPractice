public class TestOutput
{ 
	public static void main(String[] args)
	{
		// This output does not go to new line
		System.out.print("Hello my ");
		System.out.println("cousin Vinny");
		// This will print in two lines
		System.out.println("What is \n wrong with this?");
		// This combines the two text
		System.out.println("Algorithm of the year" + " Shock & regret");
		// This prints "
		System.out.println("Printing \" and \" ");
		// This prints \
		System.out.println("Printing \\");
	}
}